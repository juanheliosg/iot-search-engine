{
  "type": "kafka",
  "dataSchema": {
    "dataSource": "tseriesdb",
    "timestampSpec": {
      "column": "timestamp",
      "format": "iso"
    },
    "dimensionsSpec": {
      "dimensions": [
          {"type": "string", "name": "seriesID" },
        {"type":  "string", "name":  "city"},
        {"type":  "string", "name":  "region"},
        {"type":  "string", "name":  "country"},
        {"type":  "string", "name":  "address"},
        {"type":  "string", "name":  "name"},
        {"type":  "string", "name":  "unit"},
        {"type":  "string", "name":  "sampling_unit"},
        {"type":  "string", "name":  "measure_desc"},
        {"type":  "string", "name":  "measure_name"},
        {"type":  "string", "name":  "tags"},
        {"type":  "string", "name":  "description"},
        {"type": "string", "name": "sensorID"},
        {"type": "string", "name":  "long"},
        {"type": "string", "name":  "lat"},
          {"type": "long", "name": "measureID" },
          {"type": "string", "name": "sourceID" }
      ],
      "spatialDimensions": [{
        "dimName": "coordinates",
        "dims": ["lat","long"]
      }]
    },
    "metricsSpec": [
      {
        "name": "count",
        "type": "count"
      },
      {
        "name": "measure",
        "fieldName": "measure",
        "type": "doubleSum"
      },
      {
        "name": "value_min",
        "fieldName": "measure",
        "type": "doubleMin"
      },
      {
        "name": "value_max",
        "fieldName": "measure",
        "type": "doubleMax"
      }
    ],
    "granularitySpec": {
      "type": "uniform",
      "segmentGranularity": "HOUR",
      "queryGranularity": "NONE",
      "rollup": false 
    }
  },
  "ioConfig": {
    "topic": "tseries",
    "inputFormat": {
      "type": "json"
    },
    "consumerProperties": {
      "bootstrap.servers": "kafka:9092"
    },
    "taskCount": 1,
    "replicas": 1,
    "taskDuration": "PT1H"
  },
  "tuningConfig": {
    "type": "kafka",
    "maxRowsPerSegment": 5000000
  }
}
