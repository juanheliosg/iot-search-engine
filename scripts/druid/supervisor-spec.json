{
  "type": "kafka",
  "dataSchema": {
    "dataSource": "tseriesdb",
    "timestampSpec": {
      "column": "timestamp",
      "format": "auto"
    },
    "dimensionsSpec": {
      "dimensions": [
          {"type": "long", "name": "seriesID" },
          {"type": "long", "name": "measureID" },
          {"type": "long", "name": "sourceID" },
          {"type": "string", "name": "name" } 
      ],
      "dimensionExclusions": [
        "timestamp",
        "measure"
      ]
    },
    "metricsSpec": [
      {
        "name": "count",
        "type": "count"
      },
      {
        "name": "value_sum",
        "fieldName": "measure",
        "type": "doubleSum"
      },
      {
        "name": "value_min",
        "fieldName": "measure",
        "type": "doubleMin"
      },
      {
        "name": "value_max",
        "fieldName": "measure",
        "type": "doubleMax"
      }
    ],
    "granularitySpec": {
      "type": "uniform",
      "segmentGranularity": "HOUR",
      "queryGranularity": "NONE"
    }
  },
  "ioConfig": {
    "topic": "tseries",
    "inputFormat": {
      "type": "json"
    },
    "consumerProperties": {
      "bootstrap.servers": "kafka:9092"
    },
    "taskCount": 1,
    "replicas": 1,
    "taskDuration": "PT1H"
  },
  "tuningConfig": {
    "type": "kafka",
    "maxRowsPerSegment": 5000000
  }
}
